            <font>
                <Font name="BebasNeue" size="29.0" />
            </font>
            <fill>
                <LinearGradient endX="1.0" endY="0.03816793893129769" startX="1.0" startY="1.0">
                    <stops>
                        <Stop color="#b5d1e5" />
                        <Stop color="WHITE" offset="1.0" />
                    </stops>
                </LinearGradient>
            </fill>
        </Text>
        <Text fill="#fffcfc" layoutX="315.0" layoutY="299.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome, employee">
            <font>
                <Font name="BebasNeue" size="81.0" />
            </font>
         <stroke>
            <LinearGradient endX="1.0" startY="1.0">
               <stops>
                  <Stop color="#abc3ff" />
                  <Stop color="#579c97" offset="1.0" />
               </stops>
            </LinearGradient>
         </stroke>
        </Text>
    </children>
