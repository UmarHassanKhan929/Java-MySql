<AnchorPane prefHeight="600.0" prefWidth="950.0" style="-fx-background-color: #222;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainCodeBase.EmployeePanelController">
    <children>
        <ImageView fitHeight="602.0" fitWidth="765.0" layoutX="185.0" pickOnBounds="true">
            <image>
                <Image url="@../image/loginBGsmall.png" />
            </image>
        </ImageView>
        <Button fx:id="edit_dtls_btn" layoutX="19.0" layoutY="327.0" mnemonicParsing="false" onAction="#editPersonalBtCode" prefHeight="25.0" prefWidth="139.0" text="Edit Details" />
        <Button fx:id="gnt_pay_btn" layoutX="21.0" layoutY="451.0" mnemonicParsing="false" onAction="#GeneratePayslip" prefHeight="25.0" prefWidth="139.0" text="Generate Payslip" />
        <Button fx:id="chk_dept_info_btn" layoutX="21.0" layoutY="411.0" mnemonicParsing="false" onAction="#viewEmpDptDeets" prefHeight="25.0" prefWidth="139.0" text="Check Department Info" />
        <Button fx:id="loan_info_btn" layoutX="19.0" layoutY="370.0" mnemonicParsing="false" onAction="#viewLoanBtn" prefHeight="25.0" prefWidth="139.0" text="Loan Info" />
