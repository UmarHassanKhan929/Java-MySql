            <font>
                <Font name="BebasNeue" size="36.0" />
            </font>
         <fill>
            <LinearGradient endX="1.0" startY="1.0">
               <stops>
                  <Stop color="#abc3ff" />
                  <Stop color="#87c7c2" offset="1.0" />
               </stops>
            </LinearGradient>
         </fill>
        </Text>
        <ImageView fitHeight="140.0" fitWidth="145.0" layoutX="14.0" layoutY="36.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../image/employee.png" />
            </image>
            <effect>
                <Shadow blurType="ONE_PASS_BOX" color="WHITE" />
            </effect>
        </ImageView>
        <Button fx:id="prsn_info_btn" layoutX="21.0" layoutY="245.0" mnemonicParsing="false" onAction="#viewInfoBtn" prefHeight="25.0" prefWidth="139.0" text="Personal Info" />
        <Button fx:id="prsn_logindet_btn" layoutX="21.0" layoutY="288.0" mnemonicParsing="false" onAction="#loginDetailBt" prefHeight="25.0" prefWidth="139.0" text="Update Login Info" />
